<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEBE_VDBAdvectPoints'>
        <attribute name='PointsInput' type='maya.vectorArray'>
            <label>PointsInput</label>
        </attribute>
        <attribute name='PointsOut' type='maya.vectorArray'>
            <label>PointsOut</label>
        </attribute>
        <attribute name='VdbInputVelocity' type='maya.OpenVDBData'>
            <label>VdbInputVelocity</label>
        </attribute>                
        <attribute name='VdbInputClosestPoint' type='maya.OpenVDBData'>
            <label>VdbInputClosestPoint</label>
        </attribute>
    </template>
    <view name='NEDefault' template='NEBE_VDBAdvectPoints'>
        <property name='PointsInput'/>
        <property name='PointsOut'/>
        <property name='VdbInputVelocity'/>
        <property name='VdbInputClosestPoint'/>
    </view>
</templates>