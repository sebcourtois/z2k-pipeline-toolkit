<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEBE_VDBCombine'>
        <attribute name='VdbInputA' type='maya.OpenVDBData'>
            <label>VdbInputA</label>
        </attribute>
        <attribute name='VdbInputB' type='maya.OpenVDBData'>
            <label>VdbInputB</label>
        </attribute>
        <attribute name='VdbOutput' type='maya.OpenVDBData'>
            <label>VdbOutput</label>
        </attribute>
    </template>
    <view name='NEDefault' template='NEBE_VDBCombine'>
        <property name='VdbInputA'/>
        <property name='VdbInputB'/>
        <property name='VdbOutput'/>
    </view>
</templates>