<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEBE_VDBRay'>
        <attribute name='Positions' type='maya.vectorArray'>
            <label>Positions</label>
        </attribute>
        <attribute name='Distances' type='maya.doubleArray'>
            <label>Distances</label>
        </attribute>
        <attribute name='MeshInput' type='maya.mesh'>
            <label>MeshInput</label>
        </attribute>                
        <attribute name='MeshOut' type='maya.mesh'>
            <label>MeshOut</label>
        </attribute>
        <attribute name='VdbInput' type='maya.OpenVDBData'>
            <label>VdbInput</label>
        </attribute>        
    </template>
    <view name='NEDefault' template='NEBE_VDBRay'>
        <property name='Positions'/>
        <property name='Distances'/>
        <property name='MeshInput'/>
        <property name='MeshOut'/>
        <property name='VdbInput'/>
    </view>
</templates>