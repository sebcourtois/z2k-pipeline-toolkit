<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEBE_VDBActivate'>
		<attribute name='VdbInputSource' type='maya.OpenVDBData'>
			<label>VdbInputSource</label>
		</attribute>
		<attribute name='VdbInputReference' type='maya.OpenVDBData'>
			<label>VdbInputReference</label>
		</attribute>
		<attribute name='VdbOutput' type='maya.OpenVDBData'>
			<label>VdbOutput</label>
		</attribute>		
	</template>
	<view name='NEDefault' template='NEBE_VDBActivate'>
		<property name='VdbInputSource'/>
		<property name='VdbInputReference'/>
		<property name='VdbOutput'/>
	</view>
</templates>
