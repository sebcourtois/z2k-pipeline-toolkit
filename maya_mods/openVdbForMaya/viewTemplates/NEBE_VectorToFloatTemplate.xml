<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEBE_VectorToFloat'>
		<attribute name='VectorInput' type='maya.vectorArray'>
			<label>VectorInput</label>
		</attribute>
		<attribute name='OutX' type='maya.doubleArray'>
			<label>OutX</label>
		</attribute>
		<attribute name='OutY' type='maya.doubleArray'>
			<label>OutY</label>
		</attribute>
		<attribute name='OutZ' type='maya.doubleArray'>
			<label>OutZ</label>
		</attribute>		
	</template>
	<view name='NEDefault' template='NEBE_VectorToFloat'>
		<property name='VectorInput'/>
		<property name='OutX'/>
		<property name='OutY'/>
		<property name='OutZ'/>
	</view>
</templates>
