<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEBE_VDBToMayaFluid'>
        <attribute name='VdbInput' type='maya.OpenVDBData'>
            <label>VdbInput</label>
        </attribute>
        <attribute name='PlayFromCashe' type='maya.bool'>
            <label>PlayFromCashe</label>
        </attribute>        
        <attribute name='Translate' type='maya.float3'>
            <label>Translate</label>
        </attribute>
        <attribute name='Rotate' type='maya.float3'>
            <label>Rotate</label>
        </attribute>  
        <attribute name='Scale' type='maya.float3'>
            <label>Scale</label>
        </attribute>                                 
    </template>
    <view name='NEDefault' template='NEBE_VDBToMayaFluid'>
        <property name='VdbInput'/>
        <property name='PlayFromCashe'/>
        <property name='Translate'/>
        <property name='Rotate'/>
        <property name='Scale'/>        
    </view>
</templates>