<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEBE_SetPoints'>
        <attribute name='MeshInput' type='maya.mesh'>
            <label>MeshInput</label>
        </attribute>
        <attribute name='MeshOut' type='maya.mesh'>
            <label>MeshOut</label>
        </attribute>
        <attribute name='PositionInput' type='maya.vectorArray'>
            <label>PositionInput</label>
        </attribute>
    </template>
    <view name='NEDefault' template='NEBE_SetPoints'>
        <property name='MeshInput'/>
        <property name='MeshOut'/>
        <property name='PositionInput'/>
    </view>
</templates>
