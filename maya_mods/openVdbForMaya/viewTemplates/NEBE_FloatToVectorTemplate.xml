<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEBE_FloatToVector'>
		<attribute name='VectorOut' type='maya.vectorArray'>
			<label>VectorOut</label>
		</attribute>
		<attribute name='InputX' type='maya.doubleArray'>
			<label>InputX</label>
		</attribute>
		<attribute name='InputY' type='maya.doubleArray'>
			<label>InputY</label>
		</attribute>
		<attribute name='InputZ' type='maya.doubleArray'>
			<label>InputZ</label>
		</attribute>		
	</template>
	<view name='NEDefault' template='NEBE_FloatToVector'>
		<property name='VectorOut'/>
		<property name='InputX'/>
		<property name='InputY'/>
		<property name='InputZ'/>
	</view>
</templates>
