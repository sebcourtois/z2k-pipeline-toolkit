
<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='AEaiWireframe'>
		<attribute name='edgeType' type='maya.int'>
			<label>Edge Type</label>
		</attribute>
		<attribute name='fillColor' type='maya.float3'>
			<label>Fill Color</label>
		</attribute>
		<attribute name='lineColor' type='maya.float3'>
			<label>Line Color</label>
		</attribute>
		<attribute name='lineWidth' type='maya.float'>
			<label>Line Width</label>
		</attribute>
		<attribute name='rasterSpace' type='maya.bool'>
			<label>Raster Space</label>
		</attribute>
	</template>
	<view name='Lookdev' template='AEaiWireframe'>
		<description>View optimized for lookdev workflows</description>
		<group name='wireframe_attributes'>
			<label>Wireframe Attributes</label>
			<property name='edgeType'/>
			<property name='fillColor'/>
			<property name='lineColor'/>
			<property name='lineWidth'/>
			<property name='rasterSpace'/>
		</group>
	</view>
</templates>
