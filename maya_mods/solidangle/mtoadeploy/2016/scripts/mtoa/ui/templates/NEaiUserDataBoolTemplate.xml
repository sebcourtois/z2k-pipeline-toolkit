
<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiUserDataBool'>
		<attribute name='outValue' type='maya.bool'>
			<label>Out Value</label>
		</attribute>
		<attribute name='defaultValue' type='maya.bool'>
			<label>Default Value</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEaiUserDataBool'>
		<property name='outValue'/>
