
<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiBump2d'>
		<attribute name='bumpMap' type='maya.float'>
			<label>Bump Map</label>
		</attribute>
		<attribute name='bumpHeight' type='maya.float'>
			<label>Bump Height</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEaiBump2d'>
		<property name='bumpMap'/>
		<property name='bumpHeight'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEaiBump2d'>
		<property name='outColor'/>
	</view>
</templates>
