
<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='AEaiUserDataColor'>
		<attribute name='colorAttrName' type='maya.string'>
			<label>Color Attr Name</label>
		</attribute>
		<attribute name='defaultValue' type='maya.float3'>
			<label>Default Value</label>
		</attribute>
	</template>
	<view name='Lookdev' template='AEaiUserDataColor'>
		<description>View optimized for lookdev workflows</description>
		<group name='userdata_attributes'>
			<label>User Data Attributes</label>
			<property name='colorAttrName'/>
			<property name='defaultValue'/>
		</group>
	</view>
</templates>
