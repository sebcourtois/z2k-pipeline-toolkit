
<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaiUserDataColor'>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='defaultValue' type='maya.float3'>
			<label>Default Value</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEaiUserDataColor'>
		<property name='outColor'/>
		<property name='defaultValue'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEaiUserDataColor'>
		<property name='outColor'/>
	</view>
</templates>
