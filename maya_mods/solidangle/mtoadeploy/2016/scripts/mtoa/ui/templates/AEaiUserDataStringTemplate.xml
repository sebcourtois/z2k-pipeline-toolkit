
<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='AEaiUserDataString'>
		<attribute name='stringAttrName' type='maya.string'>
			<label>String Attr Name</label>
		</attribute>
		<attribute name='defaultValue' type='maya.string'>
			<label>Default Value</label>
		</attribute>
	</template>
	<view name='Lookdev' template='AEaiUserDataString'>
		<description>View optimized for lookdev workflows</description>
		<group name='userdata_attributes'>
			<label>User Data Attributes</label>
			<property name='stringAttrName'/>
			<property name='defaultValue'/>
		</group>
	</view>
</templates>
